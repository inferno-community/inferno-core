{
  "name": "inferno-web-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://127.0.0.1:3000",
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/lab": "^5.0.0-alpha.58",
    "@mui/material": "^5.2.2",
    "@mui/styles": "^5.2.2",
    "js-yaml": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "web-vitals": "^0.2.4",
    "yaml": "^1.10.2",
    "zustand": "^3.7.2"
  },
  "scripts": {
    "start": "webpack serve --config ./webpack.config.js --mode=development",
    "build": "webpack --config ./webpack.config.js --mode=production",
    "build:analyze": "ANALYZE_BUNDLE=static webpack --config ./webpack.config.js --mode=production",
    "puma": "bundle exec puma",
    "redis": "docker run -p 6379:6379 redis",
    "sidekiq": "bundle exec sidekiq -r ./worker.rb",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run puma\" \"npm run redis\" \"npm run sidekiq\"",
    "test": "vitest run",
    "watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint './client/src/**/*.{ts,tsx}'",
    "lint:fix": "tsc --noEmit && eslint './client/src/**/*.{ts,tsx}' --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^12.1.10",
    "@types/js-yaml": "^4.0.5",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitest/coverage-c8": "^0.22.1",
    "babel-loader": "^8.2.2",
    "babel-jest": "^27.5.1",
    "babel-preset-jest": "^27.5.1",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.5.0",
    "react-markdown": "^7.1.0",
    "remark-gfm": "^3.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.4.4",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.22.0",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-manifest-plugin": "^4.1.1",
    "webpack-nano": "^1.1.1"
  },
  "main": "webpack.config.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.mitre.org:inferno/inferno-2.git"
  },
  "keywords": [],
